#Check if a number is perfect number

num = int(input())
sumi = 0
for i in range(1,num):
  if num%i==0:
    sumi+=i
if sumi == num:
  print("yes it is a perfect number ")
else:
  print("No it is not a perfect number")
  
  
# with function 

def perfect(num):
  sumi = 0
  for i in range(1,num):
    if num%i==0:
      sumi+=i
  if sumi == num:
    return "yes it is a perfect number "
  else:
    return "No it is not a perfect number"
  
num = int(input())
print(perfect(num))





